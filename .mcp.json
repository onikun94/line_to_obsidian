{
  "mcpServers": {
    "serena": {
      "type": "stdio",
      "command": "uvx",
      "args": [
        "--from",
        "git+https://github.com/oraios/serena",
        "serena",
        "start-mcp-server",
        "--context",
        "ide-assistant",
        "--project",
        ".",
        "--enable-gui-log-window",
        "false",
        "--enable-web-dashboard",
        "false"
      ],
      "env": {}
    },
    "kiri": {
      "command": "npx",
      "args": [
        "kiri-mcp-server@latest",
        "--repo",
        ".",
        "--db",
        ".kiri/index.duckdb",
        "--watch"
      ]
    }
  }
}
